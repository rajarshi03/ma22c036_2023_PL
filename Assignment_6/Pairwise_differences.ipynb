   "source": [
    "from itertools import product\n",
    "\n",
    "\n",
    "n=len(x)\n",
    "D=np.zeros((n,n))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "for i,j in product(range(n),range(n)): D[i][j]=x[i]-x[j] "
